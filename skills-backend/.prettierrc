{
  "printWidth": 150,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "insertFinalNewline": true,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "trailingComma": "none",
        "proseWrap": "preserve"
      }
    }
  ]
}
